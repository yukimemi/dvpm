{
  "name": "@yukimemi/dvpm",
  "version": "9.1.0",
  "exports": "./mod.ts",
  "tasks": {
    "check": "deno check dvpm.ts git.ts logger.ts mod.ts plugin.ts types.ts util.ts scripts/ tests/",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "test": "deno run -A scripts/test_runner.ts",
    "ci": "deno task check && deno task lint && deno task fmt --check && deno publish --dry-run --allow-dirty && deno task test"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": false,
    "exclude": ["README.md"]
  },
  "lock": false,
  "exclude": [
    ".test_cache"
  ],
  "imports": {
    "@core/asyncutil": "jsr:@core/asyncutil@^1.2.0",
    "@denops/std": "jsr:@denops/std@^8.2.0",
    "@denops/test": "jsr:@denops/test@^4.0.0",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/fs": "jsr:@std/fs@^1.0.21",
    "@std/log": "jsr:@std/log@^0.224.14",
    "@std/path": "jsr:@std/path@^1.1.4",
    "@std/streams": "jsr:@std/streams@^1.0.16",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "@octokit/rest": "npm:@octokit/rest@^22.0.1",
    "arktype": "npm:arktype@^2.1.29"
  }
}
